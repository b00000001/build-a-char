<div class="column is-6">
  <div class="card">
        <h2 class="title p-2">{{name}}</h2>
    <div class="columns">
      <ul class="media-content is-6 p-4">
        <li class="p-2">Race: {{race}}</li>
        <li class="p-2">Gender: {{gender}}</li>
        <li class="p-2">Class: {{class.name}}</li>
      </ul>
      <ul class="media-content is-6 p-4">

        <li class="p-2">Strength:{{class.strength}}</li>

        <li class="p-2">Dexterity:{{class.dexterity}}</li>

        <li class="p-2">Intelligence:{{class.intelligence}}</li>

        <li class="p-2">Charisma:{{class.charisma}}</li>

        <li class="p-2">Constitution:{{class.constitution}}</li>
      </ul>

    </div>
    <div class="column is-1-desktop p-2">
      <div class="has-text-centered p-2">
        <button class="button is-medium p-2 is-rounded is-danger" id="deleteCharacter" data-id="{{id}}">Delete</button>
      </div>
    </div>
  </div>
</div>

<script src="/js/delete.js"></script>